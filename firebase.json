{
  "functions": {
    "source": ".output/server",
    "runtime": "nodejs18"
  },
  "hosting": {
    "public": ".output/public",
    "site": "med-doc-parser",
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "function": {
          "functionId": "server",
          "region": "asia-southeast1",  
          "pinTag": true          
        }
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}